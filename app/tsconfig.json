    "./../typings/filewriter/filewriter.d.ts",
    "./../typings/q/Q.d.ts",
    "./../typings/threejs/three.d.ts",
    "./../typings/tsd.d.ts",
    "./../typings/webcomponents.js/webcomponents.js.d.ts",
    "./../local_typings/chrome_serial.d.ts",
    "./../local_typings/threejs-extended.d.ts",
    "./../bower_components/polymer-ts/polymer-ts.d.ts",
    "./elements/fab-menu/fab-menu.ts",
    "./elements/fab-radial/fab-radial.ts",
    "./elements/layer-height/layer-height.ts",
    "./elements/microtome-app/microtome-app.ts",
    "./elements/pv-view/pv-view.ts",
    "./elements/shared-animatable/shared-animatable.ts",
    "./elements/slice-preview/slice-preview.ts",
    "./scripts/actions.ts",
    "./scripts/app.ts",
    "./scripts/background.ts",
    "./scripts/core_threed.ts",
    "./scripts/printer_config.ts",
    "./scripts/projector.ts",
    "./scripts/serial-support.ts",
    "./scripts/slicer_camera_nav.ts",
    "./scripts/slicer.ts",
    "./scripts/units.ts"
  ],
  "exclude": [],
  "atom": {
    "rewriteTsconfig": true
  }
