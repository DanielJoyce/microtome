    "./elements/layer-height/layer-height.ts",
    "./elements/microtome-app/microtome-app.ts",
    "./elements/pv-view/pv-view.ts",
    "./elements/shared-animatable/shared-animatable.ts",
    "./elements/slice-preview/slice-preview.ts",
    "./scripts/actions.ts",
    "./scripts/advanced_slicer.ts",
    "./scripts/app.ts",
    "./scripts/background.ts",
    "./scripts/core_threed.ts",
    "./scripts/printer_config.ts",
    "./scripts/projector.ts",
    "./scripts/serial-support.ts",
    "./scripts/slicer_camera_nav.ts",
    "./scripts/slicer.ts",
    "./scripts/units.ts"
