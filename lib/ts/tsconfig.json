{
  "compilerOptions": {
    "experimentalDecorators": true,
    "noImplicitAny": true,
    "removeComments": true,
    "preserveConstEnums": true,
    "target": "es5",
    "sourceMap": true,
    "newLine": "LF",
    "module": "es6",
    "declaration": true
  },
  "compileOnSave": false,
  "buildOnSave": false,
  "include": [
    "**/*.ts"
  ]
}