{
  "compilerOptions": {
    "experimentalDecorators": true,
    "noImplicitAny": true,
    "removeComments": true,
    "preserveConstEnums": true,
    "target": "es5",
    "outFile": "./../../build/lib/js/slicer-lib.js",
    "sourceMap": true,
    "newLine": "LF",
    "module": "amd",
    "declaration": true
  },
  "compileOnSave": false,
  "buildOnSave": false,
  "filesGlob": [
    "../../typings/index.d.ts",
    "./**/*.ts",
    "../../node_modules/@types/jszip/*.d.ts",
    "../../node_modules/@types/core-js/*.d.ts"
  ],
  "atom": {
    "rewriteTsconfig": true
  },
  "files": [
    "../../typings/index.d.ts",
    "./advanced_slicer.ts",
    "./core_threed.ts",
    "./printer_config.ts",
    "./projector.ts",
    "./toZipSlicer.ts",
    "./units.ts",
    "../../node_modules/@types/jszip/index.d.ts",
    "../../node_modules/@types/core-js/index.d.ts"
  ]
}
