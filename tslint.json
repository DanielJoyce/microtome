{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {
    },
    "rules": {
        "no-unused-expression": [true, "allow-new"],
        "no-console": false,
        "interface-name": [true, "never-prefix"],
        "variable-name":[
          true,
          "check-format",
          "allow-leading-underscore"
        ],
        "max-classes-per-file": false
    },
    "rulesDirectory": []
}
